var utils = require ('utils');

exports.name = function(){
  var d = new Drone(utils.getPlayerPos(self));
  d.box(blocks.stone, 3, 3, 3);
  d.fwd(1);
  d.right(1);
};

//var utils = require ('utils');
exports.haus = function(){
for(var i=0; 1 < 4; i++){
    d.box(blocks.stone,6-(i*2),1,6-(i*2));
    d.right(1);
    d.fwd(1);
    d.up(1);

}}